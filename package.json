{
  "name": "nuxt-hono-starter",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=24.11.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "prettier --write . && eslint . --fix",
    "preview": "pnpm build && node scripts/flatten-wrangler.js staging && wrangler dev --local",
    "preview:remote": "pnpm build && node scripts/flatten-wrangler.js staging && set NODE_TLS_REJECT_UNAUTHORIZED=0&& wrangler dev",
    "deploy": "pnpm build && wrangler deploy",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@nuxtjs/i18n": "10.2.0",
    "@quasar/extras": "1.17.0",
    "hono": "4.10.5",
    "inversify": "7.10.4",
    "nuxt": "4.2.1",
    "nuxt-quasar-ui": "2.1.13",
    "quasar": "2.18.6",
    "reflect-metadata": "0.2.2",
    "vue": "3.5.24",
    "vue-router": "4.6.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251113.0",
    "@nuxt/eslint": "1.10.0",
    "@types/node": "24.10.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "nitro-cloudflare-dev": "0.2.2",
    "nitropack": "2.12.9",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "wrangler": "4.48.0"
  }
}
